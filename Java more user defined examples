public class Sample {
    public static void main(String[] args) {
        add(2,1);
        
    }
    public static void add(int a, int b){
        int sum;
        sum = a+b;
        System.out.println(sum);
        if (sum<5){
        System.out.println("Positive");
        return;
        }
        System.out.println("N/A");
    }
}
